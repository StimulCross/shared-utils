{
	"name": "@stimulcross/shared-utils",
	"publishConfig": {
		"access": "public"
	},
	"version": "0.2.2",
	"description": "A set of shared utils for personal use.",
	"repository": "git@github.com:StimulCross/shared-utils.git",
	"author": "Stimul Cross <stimulcross@gmail.com>",
	"license": "MIT",
	"main": "lib",
	"module": "es",
	"types": "lib",
	"exports": {
		".": {
			"require": "./lib/index.js",
			"import": "./es/index.mjs",
			"types": "./lib/index.d.ts"
		}
	},
	"files": [
		"es",
		"lib",
		"LICENSE"
	],
	"scripts": {
		"lint": "eslint --ext js,ts",
		"prettier:check": "prettier --check .",
		"prettier:fix": "prettier --write .",
		"build": "tsukuru",
		"rebuild": "tsukuru --clean",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.1.2",
		"@stimulcross/commitlint-config": "^1.3.0",
		"@stimulcross/eslint-config-node": "^1.3.0",
		"@stimulcross/eslint-config-typescript": "^1.3.0",
		"@stimulcross/prettier-config": "^1.3.0",
		"@types/node": "^18.11.4",
		"eslint": "8.26.0",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"prettier": "^2.7.1",
		"ts-node": "^10.9.1",
		"tslib": "^2.4.0",
		"tsukuru": "^0.8.0-pre.7",
		"typescript": "4.6.3"
	}
}
